package array;

import java.util.Scanner;

public class SearchElement {
    public static void main(String[] args) {
    Scanner sc=new Scanner(System.in);
       int size=sc.nextInt();
       int []arr=new int[size];
       for(int i=0;i<size;i++){
        arr[i]=sc.nextInt();
       }
       int x=sc.nextInt();
       for(int i=0;i<size;i++){
        if(arr[i]==x){
            System.out.println("x found at index:"+i);
        }
       }
}
}

output:=5
1 2 3 9 6
9 
3
