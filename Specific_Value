package array;

public class SpecificValue {
    public static boolean contains(int[] A,int item){
        for(int n:A){
            if(item==n){
                return true;
            }
        }
        return false;
    }
    public static void main(String[] args) {
        int[] A={1,20,45,14,23,89,72,48,16,95};
        System.out.println(contains(A,23));
        }
}

