package array;

public class Commonelements {
    public static void main(String[] args) {
        int[] ar1={1,2,6,5,4,7};
        int[] ar2={8,6,4,0,3,9};
        int x;
        for(int i=0;i<ar1.length;i++){
            
            for(int j=0;j<ar2.length;j++){
                if(ar1[i]==ar2[j]){
                    System.out.println(ar1[i]);
                }
            }
        }
    }
}

