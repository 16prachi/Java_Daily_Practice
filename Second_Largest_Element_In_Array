package array;

import java.util.Arrays;

public class SecondLargest {
    public static void main(String[] args) {
        int[] ar={1,5,6,3,4};
        Arrays.sort(ar);
        int i=ar.length-1;
        while(ar[i]==ar[ar.length-1]){
            i--;
        }
        System.out.println(ar[i]);
    }
    
}

