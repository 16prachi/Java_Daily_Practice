import java.util.Arrays;

public class CopyArrayQ7 {
    public static void main(String[] args) {
        int ar[]={1,5,6,2,3,4};
        int[] newAr =new int[6];
        for(int i=0;i<ar.length;i++){
            newAr[i]=ar[i];
            }
            System.out.println(Arrays.toString(newAr));
        }
    }


