package array;

import java.util.Arrays;

public class secondSmallest {
     public static void main(String[] args) {
        int[] ar={1,5,6,3,4};
        Arrays.sort(ar);
        int i=0;
        while(ar[i]==ar[ar.length+1]){
            i++;
        }
        System.out.println(ar[i]);
    }
    
}



